{
  "servers": {
    "example": {
      "enabled": false,
      "command": "node",
      "args": ["index.js"],
      "env": {},
      "timeout": 30000,
      "maxInstances": 3,
      "description": "Example configuration for stdio MCP server"
    },
    "example-calculator": {
      "enabled": true,
      "description": "Simple calculator MCP server for testing",
      "timeout": 10000,
      "maxInstances": 2
    },
    "zen-mcp-server": {
      "enabled": true,
      "command": "bash",
      "args": ["start-zen.sh"],
      "env": {
        "PYTHONPATH": ".",
        "LOG_LEVEL": "INFO",
        "PYTHONUNBUFFERED": "1",
        "_comment": "Requires OPENROUTER_API_KEY to function in production"
      },
      "timeout": 60000,
      "maxInstances": 1,
      "restartOnCrash": true,
      "requiresStatefulConnection": true,
      "description": "AI-powered MCP server with multiple model providers for advanced coding assistance"
    }
  },
  "defaults": {
    "timeout": 30000,
    "maxInstances": 1,
    "restartOnCrash": true,
    "startupTimeout": 10000
  }
}